{
  "name": "@codetanzania/majifix-status",
  "version": "1.4.6",
  "description": "A representation of an entity which provides a way to set flags on service requests(issues) in order to track their progress.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test mocha --require @babel/register test/**/*.spec.js",
    "integration": "grunt integration",
    "dev": "npm run start",
    "start": "BASE_PATH=./examples node ./examples/app.js",
    "seed": "BASE_PATH=./examples node ./examples/seed.js",
    "commit": "git-cz",
    "commit:all": "git add -A && git-cz",
    "changelog": "changelog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeTanzania/majifix-status.git"
  },
  "keywords": [
    "codetanzania",
    "majifix",
    "majifix-module",
    "status",
    "open311"
  ],
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    },
    {
      "name": "BenMaruchu",
      "email": "benmaruchu@gmail.com",
      "url": "https://github.com/BenMaruchu"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeTanzania/majifix-status/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/CodeTanzania/majifix-status",
  "sandbox": {
    "url": "https://majifix-status.herokuapp.com/v1/statuses"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "@benmaruchu/faker": "4.2.1",
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@commitlint/travis-cli": "8.0.0",
    "@lykmapipo/mongoose-test-helpers": "0.7.7",
    "chai": "4.2.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "generate-changelog": "1.7.1",
    "grunt": "1.0.4",
    "grunt-apidoc": "0.11.0",
    "grunt-contrib-jshint": "2.1.0",
    "grunt-contrib-watch": "1.1.0",
    "grunt-mocha-test": "0.13.3",
    "husky": "2.3.0",
    "jshint-stylish": "2.2.1",
    "lint-staged": "8.1.7",
    "mocha": "6.1.4",
    "mongoose": "5.5.12",
    "prettier": "1.17.1",
    "rimraf": "2.6.3",
    "rollup": "1.13.1",
    "rollup-plugin-json": "4.0.0",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "sinon-mongoose": "2.3.0",
    "supertest": "4.0.2"
  },
  "peerDependencies": {
    "mongoose": ">=5.5.10"
  },
  "dependencies": {
    "@codetanzania/majifix-common": ">=0.12.4",
    "@codetanzania/majifix-jurisdiction": ">=1.5.5",
    "@lykmapipo/common": "^0.19.0",
    "@lykmapipo/env": "^0.7.7",
    "@lykmapipo/express-common": ">=0.17.3",
    "async": ">=3.0.0",
    "express": ">=4.17.0",
    "lodash": ">=4.17.11",
    "moment": ">=2.24.0",
    "mongoose-geojson-schemas": ">=0.10.5",
    "mongoose-locale-schema": ">=0.3.1",
    "mongoose-rest-actions": ">=0.28.1",
    "randomcolor": ">=0.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  }
}
